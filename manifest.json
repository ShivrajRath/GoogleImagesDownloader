{
  "manifest_version": 2,
  "name": "One click high resolution google image downloader",
  "version": "1.0",
  "description": "Downloads high resolution images from a google image search result page.",
  "icons": {
    "48": "icons/icon48.png",
    "24": "icons/icon24.png"
  },
  "author": "Shivraj Rath",
  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "extension.html",
    "default_title": "One click high resolution google image downloader"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*search?q*"],
      "js": ["js/api.js", "js/content_script.js"],
      "run_at": "document_idle"
    }
  ],
  "homepage_url": "https://github.com/shivrajrath",
  "background": {
    "scripts": ["js/extension_bootstrap.js"]
  },
  "permissions": ["tabs"]
}
